<component-wrap disabled>
  <view class="container">
    <view class="topList">
      <view class="topList-info">
        <image src="{{musicTopList.topinfo.pic_album}}" class="topList-info-bg"></image>
        <view class="topList-info-content">
          <image src="{{musicTopList.topinfo.pic_album}}"></image>
          <view class="topList-info-rt">
            <view class="topList-info-title">{{musicTopList.topinfo.ListName}}</view>
            <view class="topList-info-time">第 {{musicTopList.day_of_year}} 天</view>
            <view class="topList-info-update_time">{{musicTopList.update_time}} 更新</view>
          </view>
        </view>
      </view>
      <view class="topList-list">
        <view class="topList-list-head">排行榜 共{{musicTopList.total_song_num}}首</view>
        <view class="topList-songlist">
          <block wx:for="{{musicTopList.songlist}}" wx:key="{{index}}">
            <view class="song-item" data-songmid="{{item.data.songmid}}" data-albummid="{{item.data.albummid}}" data-songname="{{item.data.songname}}" data-albumname="{{item.data.albumname}}" data-singername="{{item.data.singer[0].name}}" bindtap="musicSong">
              <view>{{item.data.songname}}-{{item.data.albumname}}</view>
              <view>
                <text wx:for="{{item.data.singer}}" wx:key="{{index}}">{{item.name}} </text>
              </view>
            </view>
          </block>
        </view>
      </view>
    </view>
  </view>
</component-wrap>
