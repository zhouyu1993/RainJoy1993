<template name='onLineTierText'>
    <view class="popover" wx:if="{{onlineData.temp_type == 1}}">
        <view class='popover_box' wx:if="{{onlineData.isShow}}">
            <view class='popover_box_one'>
                <form bindsubmit="pushFormSubmit" report-submit='true'>
                    <image class='bellImg' src="https://openapi.xiaoshentui.com/push_sdk_img/online-push_type2_title.png"></image>
                    <button class='closeTwoBox' bindtap="colseOneBox" plain="true" formType="submit">
                        <image src="https://openapi.xiaoshentui.com/push_sdk_img/online-push_type2_clone.png"></image>
                    </button>
                    <view class='box_two_top'>{{onlineData.line_title}}</view>
                    <view class='box_two_btm'>{{onlineData.line_contents}}</view>
                    <button class='check_detail_two' bindtap="atDetails" data-acdetail="{{onlineData.line_page}}" data-type="{{onlineData.msg_lp_type}}" data-msgkey="{{onlineData.msg_key}}" data-apd="{{onlineData.app_id}}" formType="submit">{{onlineData.button_content}}</button>
                </form>
            </view>
        </view>
    </view>
    <view class="popover" wx:if="{{onlineData.temp_type == 2}}">
        <view class='popover_box' wx:if="{{onlineData.isShow}}">
            <view class='popover_box_two'>
                <form bindsubmit="pushFormSubmit" report-submit='true'>
                    <view class='box_one_top bgImgBox'>
                        <image class='bgImg' src="https://openapi.xiaoshentui.com/push_sdk_img/online-push_type1_title.png"></image>
                        <text>{{onlineData.line_title}}</text>
                    </view>
                    <view class='box_one_btm btn_three_btm'>
                        <view class='box_one_btm_son'>{{onlineData.line_contents}}</view>
                    </view>
                    <button class='check_detail_three' bindtap="atDetails" data-acdetail="{{onlineData.line_page}}" data-type="{{onlineData.msg_lp_type}}" data-msgkey="{{onlineData.msg_key}}" data-apd="{{onlineData.app_id}}" formType="submit">{{onlineData.button_content}}</button>
                    <button class='threeCloseImg' bindtap="colseOneBox" plain="true" formType="submit">
                        <image src="https://openapi.xiaoshentui.com/push_sdk_img/online-push_type1_clone_3.0.png"></image>
                    </button>
                </form>
            </view>
        </view>
    </view>
    <view class="popover" wx:if="{{onlineData.temp_type == 3}}">
        <view class='popover_box' wx:if="{{onlineData.isShow}}">
            <view class='popover_box_three'>
                <form bindsubmit="pushFormSubmit" report-submit='true'>
                    <view class='box_one_top'>{{onlineData.line_title}}</view>
                    <view class='box_one_btm'>
                        <view class='box_one_btm_son'>{{onlineData.line_contents}}</view>
                    </view>
                    <button class='check_detail' bindtap="atDetails" data-acdetail="{{onlineData.line_page}}" data-type="{{onlineData.msg_lp_type}}" data-msgkey="{{onlineData.msg_key}}" data-apd="{{onlineData.app_id}}" formType="submit">{{onlineData.button_content}}</button>
                    <button class='threeCloseImg' bindtap="colseOneBox" plain="true" formType="submit">
                        <image src="https://openapi.xiaoshentui.com/push_sdk_img/online-push_type1_clone_3.0.png"></image>
                    </button>
                </form>
            </view>
        </view>
    </view>
    <view class="popover" wx:if="{{ onlineData.temp_type ==4 }}">
        <view class="popover_box" wx:if="{{ onlineData.isShow }}">
            <view class="popover_box_four">
                <form bindsubmit="pushFormSubmit" report-submit='true'>
                    <view class="online-arrow">
                        <image src="https://openapi.xiaoshentui.com/push_sdk_img/online-arrow.png" class="online-arrow-img" />
                    </view>
                    <view class="online-arrow-content">
                        <view class="box-four-tip-box">
                            <image src="https://openapi.xiaoshentui.com/push_sdk_img/box-arrow.png" />
                            <text class="box-four-box-txt">{{onlineData.line_title}}</text>
                        </view>
                        <view class="box-four-content-main">
                            <view class="box-four-step">
                                <view class="box-four-step-cir">1</view>
                                <text class="box-four-step-txt">点击右上角</text>
                                <image src="https://openapi.xiaoshentui.com/push_sdk_img/online-ctrl.png" class="wx-ctrl" />
                            </view>
                            <view class="box-four-step">
                                <view class="box-four-step-cir">2</view>
                                <text class="box-four-step-txt">选择“添加到我的小程序”</text>
                            </view>
                            <button class="box-four-btn" bindtap="colseOneBox" formType="submit">{{onlineData.button_content}}</button>
                        </view>
                    </view>
                </form>
            </view>
        </view>
    </view>
    <view class="popover" wx:if="{{ onlineData.temp_type ==5 }}">
        <view class="popover_box" wx:if="{{ onlineData.isShow }}">
            <view class="popover_box_four">
                <form bindsubmit="pushFormSubmit" report-submit='true'>
                    <view class="online-arrow">
                        <image src="https://openapi.xiaoshentui.com/push_sdk_img/online-arrow.png" class="online-arrow-img" />
                    </view>
                    <view class="online-arrow-content">
                        <view class="box-four-tip-box">
                            <image src="https://openapi.xiaoshentui.com/push_sdk_img/box-arrow-blue.png" />
                            <text class="box-four-box-txt">{{onlineData.line_title}}</text>
                        </view>
                        <view class="box-four-content-main">
                            <view class="box-four-step">
                                <view class="box-four-step-cir box-four-blue">1</view>
                                <text class="box-four-step-txt">点击右上角</text>
                                <image src="https://openapi.xiaoshentui.com/push_sdk_img/online-ctrl.png" class="wx-ctrl" />
                            </view>
                            <view class="box-four-step">
                                <view class="box-four-step-cir box-four-blue">2</view>
                                <text class="box-four-step-txt">选择“添加到我的小程序”</text>
                            </view>
                            <button class="box-four-btn" bindtap="colseOneBox" formType="submit">{{onlineData.button_content}}</button>
                        </view>
                    </view>
                </form>
            </view>
        </view>
    </view>
    <view class="popover" wx:if="{{ onlineData.temp_type ==6 }}">
        <view class="popover_box" wx:if="{{ onlineData.isShow }}">
            <view class="popover-six">
                <form bindsubmit="pushFormSubmit" report-submit='true'>
                    <image src="https://openapi.xiaoshentui.com/push_sdk_img/online-tem-6.png" class="tem-six" />
                    <text class="box-six-box-txt">{{onlineData.line_title}}</text>
                    <text class="box-six-box-con">{{onlineData.line_contents}}</text>
                    <button class="box-six-btn" bindtap="atDetails" data-acdetail="{{onlineData.line_page}}" data-type="{{onlineData.msg_lp_type}}" data-msgkey="{{onlineData.msg_key}}" data-apd="{{onlineData.app_id}}" formType="submit">我知道了</button>
                    <button class="popover-online-close" bindtap="colseOneBox" plain="true" formType="submit">
                        <image src="https://openapi.xiaoshentui.com/push_sdk_img/online-close.png" class="online-close-img" />
                    </button>
                </form>
            </view>
        </view>
    </view>
    <view class="popover" wx:if="{{ onlineData.temp_type ==7 }}">
        <view class="popover_box" wx:if="{{ onlineData.isShow }}">
            <view class="popover-seven">
                <view class="popover-sev-box">
                    <form bindsubmit="pushFormSubmit" report-submit='true'>
                        <view class="popover-sev-title-box">
                            <image class="popover-sev-title-img" src="https://openapi.xiaoshentui.com/push_sdk_img/online-tem-7.png"></image>
                            <text class="popover-sev-title">{{onlineData.line_title}}</text>
                        </view>
                        <view class="popover-sev-con">{{onlineData.line_contents}}</view>
                        <button class="box-seven-btn" bindtap="atDetails" data-acdetail="{{onlineData.line_page}}" data-type="{{onlineData.msg_lp_type}}" data-msgkey="{{onlineData.msg_key}}" data-apd="{{onlineData.app_id}}" formType="submit">{{onlineData.button_content}}</button>
                        <button class="popover-seven-close" bindtap="colseOneBox" plain="true" formType="submit">
                            <image src="https://openapi.xiaoshentui.com/push_sdk_img/online-close.png" class="online-close-img" />
                        </button>
                    </form>
                </view>
            </view>
        </view>
    </view>
    <view class="popover" wx:if="{{ onlineData.temp_type ==8 }}">
        <view class="popover_box" wx:if="{{ onlineData.isShow }}">
            <view class="popover-eight">
                <form bindsubmit="pushFormSubmit" report-submit='true'>
                    <view class="popover-eight-title-box">
                        <image class="popover-eight-title-img" src="https://openapi.xiaoshentui.com/push_sdk_img/online-tem-8.png"></image>
                        <text class="popover-eight-title">{{onlineData.line_title}}</text>
                    </view>
                    <view class="popover-eight-con">{{onlineData.line_contents}}</view>
                    <button class="box-eight-btn" bindtap="atDetails" data-acdetail="{{onlineData.line_page}}" data-type="{{onlineData.msg_lp_type}}" data-msgkey="{{onlineData.msg_key}}" data-apd="{{onlineData.app_id}}" formType="submit">{{onlineData.button_content}}</button>
                    <button class="popover-seven-close" bindtap="colseOneBox" plain="true" formType="submit">
                        <image src="https://openapi.xiaoshentui.com/push_sdk_img/online-close.png" class="online-close-img" />
                    </button>
                </form>
            </view>
        </view>
    </view>
    <view class="popover" wx:if="{{ onlineData.temp_type ==9 }}">
        <view class="popover_box" wx:if="{{ onlineData.isShow }}">
            <view class="popover-nine">
                <form bindsubmit="pushFormSubmit" report-submit='true'>
                    <view class="popover-nine-title-box">
                        <image class="popover-nine-title-img" src="https://openapi.xiaoshentui.com/push_sdk_img/online-tem-9.png" />
                        <text class="popover-nine-title">{{onlineData.line_title}}</text>
                    </view>
                    <view class="popover-nine-con-box">
                        <view class="popover-con-txt">{{onlineData.line_contents}}</view>
                        <button class="box-nine-btn" bindtap="atDetails" data-acdetail="{{onlineData.line_page}}" data-type="{{onlineData.msg_lp_type}}" data-msgkey="{{onlineData.msg_key}}" data-apd="{{onlineData.app_id}}" formType="submit">{{onlineData.button_content}}</button>
                    </view>
                    <button class="popover-seven-close" bindtap="colseOneBox" plain="true" formType="submit">
                        <image src="https://openapi.xiaoshentui.com/push_sdk_img/online-close.png" class="online-close-img" />
                    </button>
                </form>
            </view>
        </view>
    </view>
</template>
<template name='onLineTierPic'>
    <view class='popover_box' wx:if="{{onlineData.isShow}}">
        <view class='popover_box_pic'>
            <view>
                <image bindtap="atDetails" data-acdetail="{{onlineData.line_page}}" data-type="{{onlineData.msg_lp_type}}" data-msgkey="{{onlineData.msg_key}}" data-apd="{{onlineData.app_id}}" src="{{onlineData.line_url}}" class="popover_box_pic_img" mode="widthFix"
                />
                <image src="https://openapi.xiaoshentui.com/push_sdk_img/online-close.png" bindtap="colseOneBox" class="popover_box_pic_clone"></image>
            </view>
        </view>
    </view>
</template>
